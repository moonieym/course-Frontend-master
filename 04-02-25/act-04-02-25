// Una funcion que obtiene dos valores y calcula el promedio
function promedio(a, b) {
    return (a + b) / 2;
}

console.log(promedio(10, 20));

console.log(typeof 42);
// number
console.log(typeof 'Twenty20');
// string
console.log(typeof '123');
//string
console.log(typeof -68);
//number
console.log(typeof true);
//boolean
console.log(typeof FALSE);
//undefined (boolean must be lower case)
console.log(typeof 'true');
//string
console.log(typeof 0);
//number